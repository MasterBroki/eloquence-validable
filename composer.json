{
    "name": "masterbroki/eloquence-validable",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/MasterBroki/eloquence-validable/issues",
        "source": "https://github.com/MasterBroki/eloquence-validable"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "authors": [
        {
            "name": "Charles Brocchiero",
            "email": "charles.brocchiero@hec.ca",
            "role": "Developer"
        },
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "https://softonsofa.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "masterbroki/eloquence-base": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "2.3.3",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Sofa\\Eloquence\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit && ./vendor/bin/phpcs src --standard=psr2 --report=diff --colors",
        "phpcs": "./vendor/bin/phpcs src --standard=psr2 --report=diff --colors"
    }
}
